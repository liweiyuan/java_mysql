<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>spark</servlet-name>
    <servlet-class>com.tingyun.SparkServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>spark</servlet-name>
    <url-pattern>/mysql</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>hadoop</servlet-name>
    <servlet-class>com.tingyun.Hadoop</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>hadoop</servlet-name>
    <url-pattern>/oracle</url-pattern>
  </servlet-mapping>
</web-app>
